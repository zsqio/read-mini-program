.collect-wrapper {
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  background: #fff;
}
 .collect-item {
  width: 33.33%;
  display: inline-block;
  margin-bottom: 20rpx;
}
.collect-item .item-image{
  width: 160rpx;
  height: 200rpx;
  display: block;
  margin: 0 auto;
}
.collect-item .item-name {
  font-size: 28rpx;
  color: #333;
  text-align: center;
}